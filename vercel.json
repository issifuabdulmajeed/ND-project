{
  "version": 2,
  "builds": [
    {
      "src": "vercel-deploy.js",
      "use": "@vercel/node"
    },
    {
      "src": ".next/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "vercel-deploy.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
